{
    "name": "stubbles/webapp-core",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Base classes for Stubbles based web applications.",
    "require": {
        "php": ">=5.4.0",
        "stubbles/core": "~4.0",
        "stubbles/input": "~3.0"
    },
    "suggest": {
        "stubbles/xml": "~3.0",
        "stubbles/webapp-session": "~4.0"
    },
    "require-dev": {
        "stubbles/xml": "~3.0",
        "stubbles/webapp-session": "~4.0",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\webapp\\": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
